<Window x:Class="KeyLoggerWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="KeyLogger" Height="87.809" Width="240.548" StateChanged="Window_StateChanged" Icon="spy.ico" ResizeMode="CanMinimize" Background="#19040202">
    <Grid Margin="0,0,4,4">
        <Button Content="Start" HorizontalAlignment="Left" Margin="24,12,0,0" VerticalAlignment="Top" Width="83" Height="30" Command="{Binding StartCommand}"/>
        <Button Content="Stop" HorizontalAlignment="Left" Margin="126,12,0,0" VerticalAlignment="Top" Width="83" Height="30" Command="{Binding StopCommand}"/>

    </Grid>
</Window>
